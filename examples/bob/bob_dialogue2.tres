[gd_resource type="Resource" load_steps=2 format=2]

[ext_resource path="res://addons/gdiag/gdiag.gd" type="Script" id=1]

[resource]
script = ExtResource( 1 )
source = "__request__
	player_name: String
	player_hp: int
	found_secret_item: bool
	know_where_is_jack: bool
	increase_trust: func

__characters__
	Bob, Player

# Comment
[MAIN]:
	Bob if found_secret_item == true: \"You found it!!\"
	Bob if found_secret_item == false: \"Hello {{ player_name }}!\"
	Player: \"Hey.\"
	Bob: \"I'm looking for Jack. Do you know where is him?\"
		- NO, main
		- YES if know_where_is_jack == true, main
	Bob: \"Text after branching\"

[NO]:
	Player: \"I don't.\"
	Bob: \"Oh..\"

[YES]:
	Player: \"He is at the lake.\"
	Bob: increase_trust(), \"Thanks, I won't forget this.\""
