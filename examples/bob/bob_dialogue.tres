[gd_resource type="Resource" load_steps=2 format=2]

[ext_resource path="res://addons/gdiag/gdiag.gd" type="Script" id=1]

[resource]
script = ExtResource( 1 )
source = "__request__
	player_name: String
	player_hp: int
	promise: bool
	increase_trust: func
	
__characters__
	Bob, Player
	
[MAIN]:
	Bob: if player_name != \"Sanyi\", \"Hello {{ player_name }}!\"
	Bob: if player_name == \"Sanyi\", \"Hi :)\"
	Player: \"Hey.\"
	Bob: \"I'm looking for Jack. Do you know where is him?\"
		- CANNOT_TELL_YOU, if promise, main
		- NO, main
		- YES, main
	Bob: \"Text after branching\"

[CANNOT_TELL_YOU]:
	Player: \"I do, but I can't tell you! sorry..\"
	Bob: \"So you are on his side...\"
	
[NO]:
	Player: \"No..\"
	Bob: \"Oh..\"

[YES]:
	Player: \"He is at the lake.\"
	Bob: increase_trust(), \"Thanks, I won't forget this.\""
